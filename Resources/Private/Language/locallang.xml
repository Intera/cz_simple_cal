<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3locallang>
	<meta type="array">
		<type>module</type>
		<description>Language labels for the Simple calendar using Extbase extension in the FRONTEND</description>
	</meta>
	<data type="array">
		<languageKey index="default" type="array">
			<label index="tx_czsimplecal_domain_model_event">an event in an calendar</label>
			<label index="tx_czsimplecal_domain_model_event.title">The title of this event</label>
			<label index="tx_czsimplecal_domain_model_event.start_day">the day that event starts</label>
			<label index="tx_czsimplecal_domain_model_event.start_time">the time this event starts (leave blank for an allday event)</label>
			<label index="tx_czsimplecal_domain_model_event.end_day">the day this event ends (leave blank for an event on one day)</label>
			<label index="tx_czsimplecal_domain_model_event.end_time">the time this event ends</label>
			<label index="tx_czsimplecal_domain_model_event.teaser">a short teaser for this event</label>
			<label index="tx_czsimplecal_domain_model_event.description">a long description for this event</label>
			<label index="tx_czsimplecal_domain_model_event.location_name">the name of the location this event takes place in</label>
			<label index="tx_czsimplecal_domain_model_event.organizer_name">the name of the institution or person the event is organized by</label>
			<label index="tx_czsimplecal_domain_model_event.categories">categories</label>
			<label index="tx_czsimplecal_domain_model_category">a category of an event</label>
			<label index="tx_czsimplecal_domain_model_category.title">a title for this category</label>
			
			<label index="event">Event</label>
			<label index="events">Events</label>
			
			<label index="msg.noEvents">No events have been found.</label>
			
			<label index="format.monthAndYear">%B %Y</label>
			<label index="format.dayWrapper">%a, %B %e, %Y</label>
			<label index="format.show.allday">%B %e, %Y</label>
			<label index="format.show.dayTime">%B %e, %Y %H:%M</label>
			<label index="format.show.time">%H:%M</label>
			
			<!-- used for timespan generation -->
			<label index="timespan.format.sameDay">%B %e, %Y</label>
			<label index="timespan.format.sameMonth.start">%B %e</label>
			<label index="timespan.format.sameMonth.end">%e, %Y</label>
			<label index="timespan.format.sameYear.start">%B %e</label>
			<label index="timespan.format.sameYear.end">%B %e, %Y</label>
			<label index="timespan.format.else.start">%B %e, %Y</label>
			<label index="timespan.format.else.end">%B %e, %Y</label>
			<label index="timespan.from">from</label>
			<label index="timespan.to">to</label>
			<label index="timespan.on">on</label>
			
			
			<label index="time.end">end at</label>
			
			<label index="numEvents">[0]No events|[1] 1 event|[2,+Inf] ###number### events</label>
			
			<!-- labels for organizers and location -->
			<label index="organizer">Organizer</label>
			<label index="location">Location</label>
			<label index="phone">Phone</label>
			<label index="fax">Fax</label>
			<label index="mobile">Mobile</label>
			<label index="email">Email</label>
			<label index="www">WWW</label>
			<label index="title.gender.male">Mr.</label>
			<label index="title.gender.female">Mrs.</label>
				
		</languageKey>
		<languageKey index="de" type="array">
			<label index="format.monthAndYear">%B %Y</label>
			<label index="format.dayWrapper">%a, %e. %B %Y</label>
		</languageKey>
		
	</data>
</T3locallang>