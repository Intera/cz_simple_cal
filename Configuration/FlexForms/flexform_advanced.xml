<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>
                        LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:sheet.general
                    </sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <switchableControllerActions>
                        <TCEforms>
                            <label>LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.useView
                            </label>
                            <config>
                                <type>select</type>
                                <renderType>selectMultipleSideBySide</renderType>
                                <items></items>
                                <itemsProcFunc>Tx\CzSimpleCal\Utility\FlexConfig->getAllowedActions</itemsProcFunc>
                            </config>
                        </TCEforms>
                    </switchableControllerActions>

                    <view.defaultPid>
                        <TCEforms>
                            <label>
                                LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.view.defaultPid
                            </label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                                <wizards>
                                    <suggest>
                                        <type>suggest</type>
                                    </suggest>
                                </wizards>
                            </config>
                        </TCEforms>
                    </view.defaultPid>

                    <settings.override.action.filter.categories.uid>
                        <TCEforms>
                            <label>
                                LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.filterCategories
                            </label>
                            <config>
                                <type>select</type>
                                <foreign_table>tx_czsimplecal_domain_model_category</foreign_table>
                                <minitems>0</minitems>
                                <maxitems>500</maxitems>
                                <size>6</size>
                            </config>
                        </TCEforms>
                    </settings.override.action.filter.categories.uid>

                    <settings.override.action.filter.organizer>
                        <TCEforms>
                            <label>
                                LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.filterOrganizer
                            </label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>tx_czsimplecal_domain_model_address</allowed>
                                <minitems>0</minitems>
                                <maxitems>500</maxitems>
                                <size>6</size>
                            </config>
                        </TCEforms>
                    </settings.override.action.filter.organizer>

                    <settings.override.action.filter.location>
                        <TCEforms>
                            <label>
                                LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.filterLocation
                            </label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>tx_czsimplecal_domain_model_address</allowed>
                                <minitems>0</minitems>
                                <maxitems>500</maxitems>
                                <size>6</size>
                            </config>
                        </TCEforms>
                    </settings.override.action.filter.location>

                    <settings.override.action.startDate>
                        <TCEforms>
                            <label>
                                LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.startDate
                            </label>
                            <config>
                                <type>select</type>
                                <items>
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.startDate.default
                                        </numIndex>
                                        <numIndex index="1">0</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.startDate.thisYear
                                        </numIndex>
                                        <numIndex index="1">%Y</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.startDate.thisMonth
                                        </numIndex>
                                        <numIndex index="1">%B %Y</numIndex>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.startDate.thisWeek
                                        </numIndex>
                                        <numIndex index="1">%Y-W%V</numIndex>
                                    </numIndex>
                                    <numIndex index="4" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.startDate.yesterday
                                        </numIndex>
                                        <numIndex index="1">yesterday</numIndex>
                                    </numIndex>
                                    <numIndex index="5" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.startDate.today
                                        </numIndex>
                                        <numIndex index="1">today</numIndex>
                                    </numIndex>
                                    <numIndex index="6" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.startDate.now
                                        </numIndex>
                                        <numIndex index="1">now</numIndex>
                                    </numIndex>
                                    <numIndex index="7" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.startDate.tomorrow
                                        </numIndex>
                                        <numIndex index="1">tomorrow</numIndex>
                                    </numIndex>
                                    <numIndex index="8" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.startDate.nextMonth
                                        </numIndex>
                                        <numIndex index="1">%B %Y +1 month</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.override.action.startDate>

                    <settings.override.action.endDate>
                        <TCEforms>
                            <label>
                                LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.endDate
                            </label>
                            <config>
                                <type>select</type>
                                <items>
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.endDate.default
                                        </numIndex>
                                        <numIndex index="1">0</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.endDate.today
                                        </numIndex>
                                        <numIndex index="1">today 23:59:59</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.endDate.24hours
                                        </numIndex>
                                        <numIndex index="1">now +1 day -1 second</numIndex>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.endDate.tomorrow
                                        </numIndex>
                                        <numIndex index="1">tomorrow 23:59:59</numIndex>
                                    </numIndex>
                                    <numIndex index="4" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.endDate.thisWeek
                                        </numIndex>
                                        <numIndex index="1">last day of this week 23:59:59</numIndex>
                                    </numIndex>
                                    <numIndex index="5" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.endDate.thisMonth
                                        </numIndex>
                                        <numIndex index="1">last day of this month 23:59:59</numIndex>
                                    </numIndex>
                                    <numIndex index="6" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.endDate.nextMonth
                                        </numIndex>
                                        <numIndex index="1">last day of next month 23:59:59</numIndex>
                                    </numIndex>
                                    <numIndex index="7" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.endDate.thisYear
                                        </numIndex>
                                        <numIndex index="1">last day of this year 23:59:59</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.override.action.endDate>

                    <settings.override.action.maxEvents>
                        <TCEforms>
                            <label>
                                LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.maxEvents
                            </label>
                            <config>
                                <type>input</type>
                                <eval>integer</eval>
                                <checkbox>0</checkbox>
                                <default>0</default>
                                <size>4</size>
                                <max>4</max>
                            </config>
                        </TCEforms>
                    </settings.override.action.maxEvents>

                    <settings.override.action.orderBy>
                        <TCEforms>
                            <label>
                                LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.orderBy
                            </label>
                            <config>
                                <type>select</type>
                                <items>
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.orderBy.default
                                        </numIndex>
                                        <numIndex index="1">0</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.orderBy.start
                                        </numIndex>
                                        <numIndex index="1">start</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.orderBy.end
                                        </numIndex>
                                        <numIndex index="1">end</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.override.action.orderBy>

                    <settings.override.action.order>
                        <TCEforms>
                            <label>
                                LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.order
                            </label>
                            <config>
                                <type>select</type>
                                <items>
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.order.default
                                        </numIndex>
                                        <numIndex index="1">0</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.order.ascending
                                        </numIndex>
                                        <numIndex index="1">asc</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">
                                            LLL:EXT:cz_simple_cal/Resources/Private/Language/locallang_flex.xml:field.action.order.descending
                                        </numIndex>
                                        <numIndex index="1">desc</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.override.action.order>
                </el>
            </ROOT>
        </sDEF>
        <rss>
            <ROOT>
                <TCEforms>
                    <sheetTitle>RSS</sheetTitle>
                </TCEforms>

                <type>array</type>

                <el>
                    <settings.rss.upcoming.channelDescription>
                        <TCEforms>
                            <label>Anstehende Termine - Bschreibung des RSS Feeds</label>
                            <config>
                                <type>input</type>
                                <size>50</size>
                            </config>
                        </TCEforms>
                    </settings.rss.upcoming.channelDescription>

                    <settings.rss.upcoming.channelTitle>
                        <TCEforms>
                            <label>
                                Anstehende Termine - Titel des RSS Feeds überschreiben (als Standard wird die
                                Überschrift + "Anstehende Termine" verwendet)
                            </label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.rss.upcoming.channelTitle>

                    <settings.rss.latest.channelDescription>
                        <TCEforms>
                            <label>Neuste Termine - Beschreibung des RSS Feeds</label>
                            <config>
                                <type>input</type>
                                <size>50</size>
                            </config>
                        </TCEforms>
                    </settings.rss.latest.channelDescription>

                    <settings.rss.latest.channelTitle>
                        <TCEforms>
                            <label>
                                Neuste Termine - Titel des RSS Feeds überschreiben (als Standard wird die Überschrift
                                + "Neuste Termine" verwendet)
                            </label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.rss.latest.channelTitle>

                    <settings.rss.disable>
                        <TCEforms>
                            <label>RSS Feed deaktivieren</label>
                            <config>
                                <type>check</type>
                                <default/>
                            </config>
                        </TCEforms>
                    </settings.rss.disable>
                </el>
            </ROOT>
        </rss>
    </sheets>
</T3DataStructure>
