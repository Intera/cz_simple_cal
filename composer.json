{
	"name": "intera-typo3-extension/cz-simple-cal",
	"description": "A simple calendar.",
	"type": "typo3-cms-extension",
	"license": "GPL-2.0-or-later",
	"replace": {
		"typo3-ter/cz-simple-cal": "self.version"
	},
	"authors": [
		{
			"name": "Christian Zenker",
			"email": "christian.zenker@599media.de"
		},
		{
			"name": "Alexander Stehlik",
			"email": "astehlik.deleteme@intera.de",
			"homepage": "https://www.intera.de"
		}
	],
	"autoload": {
		"psr-4": {
			"Tx\\CzSimpleCal\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tx\\CzSimpleCal\\Tests\\": "Tests/"
		}
	},
	"require": {
		"php": "^7.2",
		"typo3/cms-core": "^9.3"
	},
	"require-dev": {
		"typo3/cms-fluid-styled-content": "*",
		"de-swebhosting/typo3-extension-buildtools": "dev-TYPO3_9"
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin"
	},
	"scripts": {
		"post-autoload-dump": [
			"TYPO3\\TestingFramework\\Composer\\ExtensionTestEnvironment::prepare",
			"De\\SWebhosting\\Buildtools\\ExtensionTestEnvironment::prepare"
		]
	},
	"extra": {
		"typo3/cms": {
			"app-dir": ".Build",
			"web-dir": ".Build/Web",
			"extension-key": "cz_simple_cal"
		}
	}
}
